<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-mybatis.org//OTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gdufe.cs.admin.mapper.DirectorMapper">
    <update id="update">
        update director
        <set>
            <if test="directorName != null">
                directorName=#{directorName},
            </if>
            <if test="directorIntro != null">
                directorIntro=#{directorIntro},
            </if>
            <if test="directorBirth != null">
                directorBirth=#{directorBirth}
            </if>
        </set>
        <where>
            directorId=#{directorId}
        </where>
    </update>
</mapper>
